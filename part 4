#include <iostream>
using namespace std;
int main() {
  int a,b,c;
cin >> a >> b >> c;
if (a > b) {
  if (a > c) cout << a;
  else cout << c;
}
else {
  if (b > c) cout << b;
  else cout << c;
}
cout << "\n";
int year;
cout << "enter a year:";
cin >> year;
if (year % 4==0 && year % 100 !=0)
  cout << "leap year";
else 
  cout << "not Leap year";
return 0;
}
